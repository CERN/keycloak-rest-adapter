{
    "web": {
        "issuer": "https://$KEYCLOAK_FQDN/auth/realms/master",
        "auth_uri": "https://$KEYCLOAK_FQDN/auth/realms/master/protocol/openid-connect/auth",
        "client_id": "$CLIENT_ID",
        "client_secret": "$CLIENT_SECRET",
        "redirect_uris": [
            "https://$HOSTNAME:5000/*"
